{
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.codeAnalysis.clangTidy.args": [
        "--config-file=${workspaceFolder}/.clang-tidy"
    ],
    "C_Cpp.codeAnalysis.clangTidy.enabled": false,
    "C_Cpp.codeAnalysis.exclude": {
        "**/*.h.in": true
    },
    "C_Cpp.default.compileCommands": "${workspaceFolder}/out/build/compile_commands.json",
    "C_Cpp.default.defines": [
        "LOGURU_USE_FMTLIB=1"
    ],
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/src",
        "~/.conan2/**"
    ],
    "C_Cpp.default.intelliSenseMode": "",
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.errorSquiggles": "disabled",
    "[cmake]": {
        "editor.formatOnSave": false,
        "editor.formatOnType": false
    },
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}/src",
        "~/.conan2/**"
    ],
    "C_Cpp.default.browse.databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
    "files.exclude": {
        "**/${workspaceFolder}/out": true
    },
    "search.exclude": {
        "**/${workspaceFolder}/out": true
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.formatOnSave": true
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "black-formatter.args": [
        "--line-length",
        "80"
    ],
    "chat.instructionsFilesLocations": {
        ".github": true
    },
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.chat.commitMessageGeneration.instructions": [
        {
            "text": "Use conventional commit message format. Write concise commit messages that describe the changes made rather than the implementation details."
        }
    ],
    "powershell.codeFormatting.preset": "OTBS",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
            "args": [
                "-NoExit",
                "-File",
                "${workspaceFolder}\\.vscode\\default-profile.ps1"
            ]
        }
    },
    "testMate.cpp.log.logpanel": true,
    "testMate.cpp.test.executables": "out/build/bin/Debug/*.Tests.exe",
    "testMate.cpp.test.stderrDecorator": false,
    "yaml.schemas": {
        "src/Oxygen/Core/Bindless/Spec.schema.json": [
            "**/Spec.yaml"
        ]
    },
    "yaml.validate": true,
    "cmake.sourceDirectory": "F:/projects/DroidNet/projects/Oxygen.Engine",
    "dotnet.testWindow.useTestingPlatformProtocol": true,
    "editor.rulers": [
        80,
        {
            "column": 100,
            "color": "#118baa"
        },
        {
            "column": 120,
            "color": "#ffcc00"
        },
        {
            "column": 130,
            "color": "#ff0000"
        }
    ],
    "files.associations": {
        "*.def": "cpp",
        "*.impl": "cpp",
        "*.ipp": "cpp",
